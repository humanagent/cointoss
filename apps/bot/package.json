{
  "name": "cointoss-bot",
  "version": "0.0.3",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf .turbo && rm -rf node_modules && yarn clean:lib",
    "clean:lib": "rm -rf dist",
    "dev": "tsc -w & sleep 1 && nodemon --quiet dist/index.js",
    "dev:bun": "tsc -w & sleep 1 && bun run --watch dist/index.js",
    "format": "yarn format:base -w .",
    "format:base": "prettier --ignore-path ../../.gitignore",
    "format:check": "yarn format:base -c .",
    "lint": "echo \"No linting\"",
    "start": "node dist/index.js",
    "test": "echo \"No tests\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@redis/client": "^1.6.0",
    "@xmtp/message-kit": "^1.1.7-beta.4",
    "lowdb": "^7.0.1",
    "openai": "^4.58.1",
    "uuid": "^10.0.0",
    "viem": "^2.18.7"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@types/timestring": "^6.0.5",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.3",
    "prettier-plugin-packagejson": "^2.5.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=20"
  }
}
